                    android:id="@+id/blueShieldGenerator"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?blueAllianceBackground"
                    android:textColor="?blueAllianceText"
                    android:paddingRight="4dp" />
            </TableRow>
            <TableRow
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_marginBottom="2dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_weight="@dimen/match_2020_text_weight"
                    android:gravity="start|center_vertical"
                    android:textSize="@dimen/match_2020_h2"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="?textOnBackground"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Fouls/Tech Fouls"
                    android:background="?cardBackground"
                    android:paddingLeft="32dp"/>
                <TextView
                    android:layout_weight="@dimen/match_2020_score_weight"
                    android:textSize="@dimen/match_2020_h2"
                    android:id="@+id/redFouls"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?redAllianceBackground"
                    android:textColor="?redAllianceText"
                    android:paddingRight="4dp" />
                <TextView
                    android:layout_weight="@dimen/match_2020_score_weight"
                    android:textSize="@dimen/match_2020_h2"
                    android:id="@+id/blueFouls"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?blueAllianceBackground"
                    android:textColor="?blueAllianceText"
                    android:paddingRight="4dp" />
            </TableRow>
            <TableRow
                android:id="@+id/adjustments"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_marginBottom="2dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_weight="@dimen/match_2020_text_weight"
                    android:gravity="start|center_vertical"
                    android:textSize="@dimen/match_2020_h2"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="?textOnBackground"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Adjustments"
                    android:background="?cardBackground"
                    android:paddingLeft="32dp"/>
                <TextView
                    android:layout_weight="@dimen/match_2020_score_weight"
                    android:textSize="@dimen/match_2020_h2"
                    android:id="@+id/redAdjustments"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?redAllianceBackground"
                    android:textColor="?redAllianceText"
                    android:paddingRight="4dp" />
                <TextView
                    android:layout_weight="@dimen/match_2020_score_weight"
                    android:textSize="@dimen/match_2020_h2"
                    android:id="@+id/blueAdjustments"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?blueAllianceBackground"
                    android:textColor="?blueAllianceText"
                    android:paddingRight="4dp" />
            </TableRow>
            <TableRow
                android:id="@+id/rankPoints"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_marginBottom="2dp"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_weight="@dimen/match_2020_text_weight"
                    android:gravity="start|center_vertical"
                    android:textSize="@dimen/match_2020_h1"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="?textOnBackground"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Rank Points"
                    android:background="?cardBackground"
                    android:paddingLeft="@dimen/activity_vertical_margin"/>
                <TextView
                    android:layout_weight="1"
                    android:textSize="@dimen/match_score_text"
                    android:id="@+id/redRankPoints"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?redAllianceBackground"
                    android:textColor="?redAllianceText"
                    android:paddingRight="4dp" />
                <TextView
                    android:layout_weight="1"
                    android:textSize="@dimen/match_score_text"
                    android:id="@+id/blueRankPoints"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="end|center_vertical"
                    android:background="?blueAllianceBackground"
                    android:textColor="?blueAllianceText"
                    android:paddingRight="4dp" />
            </TableRow>
        </TableLayout>

    </ScrollView>
</LinearLayout>